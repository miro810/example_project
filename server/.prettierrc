{
    "printWidth": 140,
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "never",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-organize-imports"]
}
