{
  "name": "server",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "devCompile": "tsc && ts-add-js-extension --dir=dist && cross-env NODE_ENV=development node dist/index.js",
    "devServer": "nodemon --ext \"src/**/*.ts\" --exec \"yarn run devCompile\"",
    "devServerBuild": "tsc && ts-add-js-extension --dir=dist",
    "prodServer": "cross-env NODE_ENV=production node dist/index.js",
    "pm2ServerRun": "pm2 start dist/index.js -i 1 --watch --name ShopServer",
    "pm2ServerLogs": "pm2 monit",
    "pm2ServerDelete": "pm2 delete ShopServer",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.2",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.3.1",
    "sequelize": "^6.37.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.20",
    "@types/pg": "^8.11.1",
    "@types/uuid": "^9.0.8",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-add-js-extension": "^1.6.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
